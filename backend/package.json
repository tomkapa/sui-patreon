{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "indexer": "bun --watch src/indexer.ts",
    "build": "bun build src/index.ts --outdir dist",
    "start": "NODE_ENV=production bun src/index.ts",
    "start:railway": "bash start-railway.sh",
    "start:node": "NODE_ENV=production node src/index.ts",
    "test": "bun test",
    "lint": "eslint src --ext .ts",
    "prisma:migrate": "bunx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:generate": "bunx prisma generate",
    "prisma:studio": "bunx prisma studio",
    "prisma:seed": "bun prisma/seed.ts",
    "clean": "rm -rf dist node_modules"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.934.0",
    "@mysten/seal": "0.9.4",
    "@mysten/sui": "1.45.0",
    "@prisma/client": "6.19.0",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/bun": "1.3.2",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/supertest": "6.0.3",
    "prisma": "6.19.0",
    "supertest": "7.1.4"
  }
}
